{
  "name": "rohanrk.github.io",
  "version": "1.0.0",
  "description": "Rohan's personal site",
  "scripts": {
    "tsc": "tsc",
    "build-dev": "parcel build public/index.html --no-optimize --public-url ./",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "dev": "parcel public/index.html",
    "build": "parcel build public/index.html --public-url ./",
    "deploy": "push-dir --dir=dist --branch=deploy --cleanup --verbose",
    "copy-assets": "cp -vR src/asset/pfp.png ./dist",
    "copy-markdown": "cp -vR src/posts ./dist",
    "clean": "rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohanrk/rohanrk.github.io.git"
  },
  "author": "rohanrk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rohanrk/rohanrk.github.io/issues"
  },
  "homepage": "https://github.com/rohanrk/rohanrk.github.io#readme",
  "devDependencies": {
    "@parcel/transformer-typescript-types": "latest",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router": "^5.1.20",
    "@types/node": "20.11.20",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "parcel": "^2.11.0",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "push-dir": "^0.4.1",
    "typescript": "^5.1.20"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4",
    "react-showdown": "^2.3.1"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
